<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="singleValueDialog" alignment="BOTTOM_LEFT" maxHeight="124.0" maxWidth="420.0" minHeight="124.0" minWidth="420.0" prefHeight="124.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <GridPane VBox.vgrow="ALWAYS">
      <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="70.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="24.0" prefHeight="24.0" vgrow="ALWAYS" />
      </rowConstraints>
      <children>
        <Label fx:id="itemLabel" alignment="CENTER_RIGHT" maxHeight="30.0" minHeight="30.0" minWidth="130.0" prefHeight="30.0" text="Application Name">
          <padding>
            <Insets left="10.0" right="4.0" />
          </padding>
          <GridPane.margin>
            <Insets top="6.0" />
          </GridPane.margin>
        </Label>
        <TextField fx:id="itemValue" maxHeight="30.0" minHeight="30.0" minWidth="250.0" prefHeight="30.0" GridPane.columnIndex="1">
          <GridPane.margin>
            <Insets right="10.0" top="6.0" />
          </GridPane.margin>
        </TextField>
      </children>
    </GridPane>
    <FlowPane alignment="CENTER_RIGHT" columnHalignment="RIGHT" hgap="10.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="200.0" prefWidth="200.0">
      <children>
        <Button fx:id="cancelButton" alignment="CENTER_RIGHT" cancelButton="true" contentDisplay="CENTER" mnemonicParsing="false" onAction="#cancel" text="Cancel">
          <FlowPane.margin>
            <Insets />
          </FlowPane.margin>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
        </Button>
        <Button fx:id="saveButton" defaultButton="true" mnemonicParsing="false" onAction="#save" text="Save">
          <FlowPane.margin>
            <Insets right="10.0" />
          </FlowPane.margin>
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
        </Button>
      </children>
    </FlowPane>
  </children>
  <stylesheets>
      <URL value="@../css/installmation.css" />
      <URL value="@../css/bootstrap3.css" />
  </stylesheets>
</VBox>
